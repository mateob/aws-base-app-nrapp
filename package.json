{
  "name": "aws-noc-base-api",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "start:dev": "npx sls offline start --host 0.0.0.0",
    "start:debug": "node --inspect ./node_modules/.bin/sls offline start --host 0.0.0.0",
    "test": "ts-mocha -r module-alias/register test/**/*.spec.ts",
    "test:coverage": "nyc npm run test",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "db:create": "cross-env NODE_ENV=local npx sequelize db:create",
    "db:drop": "cross-env NODE_ENV=local npx sequelize db:drop",
    "db:migrate": "cross-env NODE_ENV=local npx sequelize db:migrate",
    "db:seed": "cross-env NODE_ENV=local npx sequelize db:seed:all",
    "db:reconfigure": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "doc:openapi": "npx sls openapi generate --output config/serverless/docs/openapi.yaml"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@middy/core": "^3.6.2",
    "@middy/http-error-handler": "^4.6.5",
    "@middy/http-event-normalizer": "^4.6.5",
    "@middy/http-json-body-parser": "^3.6.2",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1490.0",
    "axios": "^1.6.0",
    "env-var": "^7.4.1",
    "firebase-admin": "^12.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.3",
    "md5": "^2.3.0",
    "module-alias": "^2.2.3",
    "pg": "^8.11.3",
    "pino": "^8.16.1",
    "sequelize": "^6.34.0",
    "serialize-error": "^11.0.2",
    "source-map-support": "^0.5.21",
    "string-format": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/chai": "^4.3.10",
    "@types/hapi__joi": "^17.1.13",
    "@types/jasmine": "^5.1.2",
    "@types/luxon": "^3.3.4",
    "@types/node": "^14.14.25",
    "@types/pg": "^8.10.9",
    "@types/pino": "^7.0.5",
    "@types/sinon": "^17.0.1",
    "@types/sinon-chai": "^3.2.12",
    "@types/string-format": "^2.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "chai": "^4.3.10",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.54",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-module-resolver": "^1.5.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jasmine": "^5.1.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-reporters": "^2.5.2",
    "jasmine-ts": "^0.4.0",
    "json-schema-to-ts": "^1.6.5",
    "mocha": "^10.2.0",
    "ngrok": "^5.0.0-beta.2",
    "nyc": "^15.1.0",
    "serverless": "^3.36.0",
    "serverless-domain-manager": "^7.1.2",
    "serverless-esbuild": "^1.48.5",
    "serverless-offline": "^13.2.1",
    "serverless-openapi-documentation": "^0.4.0",
    "serverless-plugin-resource-tagging": "^1.2.0",
    "serverless-webpack": "^5.13.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-loader": "^9.5.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.25.3",
    "typescript": "^4.1.3",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "keywords": [],
  "_moduleAliases": {
    "@function": "./src/function",
    "@core/config": "./src/core/config",
    "@constants": "./src/core/constants",
    "@entities": "./src/core/entities",
    "@enums": "./src/core/enums",
    "@factories": "./src/core/factories",
    "@interfaces": "./src/core/interfaces",
    "@middlewares": "./src/core/middlewares",
    "@repositories": "./src/core/repositories",
    "@services": "./src/core/services",
    "@validators": "./src/core/validators",
    "@types": "./src/config/types",
    "@utils": "./src/config/utils"
  }
}
